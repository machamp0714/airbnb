<div class='panel'>
  <div class='panel-body'>
    <%= image_tag avatar_url(message.user), class: "image-circle avatar-medium" %>
    <strong> <%= message.user.name %> </strong>
    <span class='pull-right'> <%= message.message_time %> </span>
    <br>
    <div class='row-space-2'>
      <%= message.content %>
    </div>
  </div>
</div>